@model IEnumerable<Lesca.Models.Clientes>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/LayoutVisor.cshtml";
}

<p><br />
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.solicitud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.volocidad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.solicitud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.volocidad)
        </td>
        <td>
            @Html.ActionLink("Detalles", "DetailsClient", new { id=item.ID })
            @Html.ActionLink("Historial", "", new { id=item.ID })
        </td>
    </tr>
}

</table>
