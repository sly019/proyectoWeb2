            @Html.DisplayFor(modelItem => item.volocidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IP_publica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IP_Privada)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
